<!DOCTYPE html>
<html>
<head>
  <title>SweetmamaDashboardPrototype</title>
  <%= stylesheet_link_tag    'user-dashboard', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar navbar-light bg-faded">
    <a class="navbar-brand" href="/">SweetMama</a>
    <ul class="nav navbar-nav">
      <li class="nav-item">
        <%= link_to 'Messages', prototype_messages_path, class: 'nav-link' %>
      </li>
    </ul>
  </nav>

<%= yield %>

</body>
</html>
