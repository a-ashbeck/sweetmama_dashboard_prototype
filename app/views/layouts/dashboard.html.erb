<!DOCTYPE html>
<html>
<head>
  <title>SweetmamaDashboardPrototype</title>
  <%= stylesheet_link_tag    'user-dashboard', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar navbar-light bg-faded">
    <a class="navbar-brand" href="/">SweetMama</a>
    <ul class="nav navbar-nav">
      <li class="nav-item">
        <%= link_to 'Messages', prototype_messages_path, class: 'nav-link' %>
      </li>
      <li class="nav-item">
        <%= link_to 'Notifications', prototype_notifications_path, class: 'nav-link' %>
      </li>
      <li class="nav-item">
        <%= link_to 'Participant Activity', prototype_participant_activities_path, class: 'nav-link' %>
      </li>
      <li class="nav-item pull-xs-right">
        <%= link_to "Sign Out", prototype_logins_path, class: "btn btn-info" %>
      </li>
    </ul>
  </nav>

  <br />

  <div class="row">
    <div class="col-xs-1"></div>
    <div class="col-xs-10"><%= yield %></div>
    <div class="col-xs-1"></div>
  </div>

</body>
</html>
